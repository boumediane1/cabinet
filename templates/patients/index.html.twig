<a href="/patients/create">Add new patient</a>

<table>
    <thead>
    <tr>
        <th>Name</th>
        <th>Birth date</th>
    </tr>
    </thead>

    <tbody>
    {% for patient in patients %}
        <tr>
            <td>{{ patient.name }}</td>
            <td>{{ patient.birthDate.format('Y/m/d') }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
